# Ignore ENV file.
.env

# Ignore dependencies.
node_modules

# Ignore compiled output (containing all emitted code and types).
dist/

# Ignore compiled output.
src/**/*.d.ts

# Ignore all compiled JavaScript files (trans-compiled from TypeScript).
**/*.js
**/*.js.map

# But DO NOT ignore jest.config.js.
!jest.config.js

# But DO NOT ignore eslint.config.js, needed by ESLint v9+.
!eslint.config.js

# ANTLR4 output directory where all output is generated to.
gen-output

# Ignore logs...
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*
.pnpm-debug.log*

*.local
